{
	"name": "Run Dataflow",
	"properties": {
		"activities": [
			{
				"name": "Load with Dataflow",
				"type": "ExecuteDataFlow",
				"dependsOn": [],
				"policy": {
					"timeout": "0.12:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"dataflow": {
						"referenceName": "Move data to dataflow parquet",
						"type": "DataFlowReference",
						"parameters": {
							"container": {
								"value": "'@{pipeline().parameters.container}'",
								"type": "Expression"
							},
							"file": {
								"value": "'@{pipeline().parameters.file}'",
								"type": "Expression"
							}
						}
					},
					"compute": {
						"coreCount": 8,
						"computeType": "General"
					},
					"traceLevel": "Fine"
				}
			}
		],
		"parameters": {
			"container": {
				"type": "string",
				"defaultValue": "captureddata"
			},
			"folder": {
				"type": "string",
				"defaultValue": "@triggerBody().folderPath"
			},
			"file": {
				"type": "string",
				"defaultValue": "@triggerBody().fileName"
			}
		},
		"annotations": []
	}
}